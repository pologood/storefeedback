        <artifactId>maven-surefire-plugin</artifactId>
        <version>2.11</version>
        <configuration>
          <argLine>-Dfile.encoding=UTF-8</argLine>
          <skip>true</skip>
        </configuration>
      </plugin>
    </plugins>
